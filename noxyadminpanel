    cmdInput.Text = ";balloon"
end)

createCmdButton("üîÑ", "inverse", UDim2.new(0.52, 0, 0, 120), function()
    cmdInput.Text = ";inverse"
end)

createCmdButton("üëÅÔ∏è", "nightvision", UDim2.new(0, 5, 0, 170), function()
    cmdInput.Text = ";nightvision"
end)

createCmdButton("üè¢", "jail", UDim2.new(0.52, 0, 0, 170), function()
    cmdInput.Text = ";jail"
end)

createCmdButton("üé≠", "control", UDim2.new(0, 5, 0, 220), function()
    cmdInput.Text = ";control"
end)

