-- üéÆ ADMIN PANEL - STEAL A BRAINROT STYLE üéÆ
local Players = game:GetService("Players")
local player = Players.LocalPlayer

pcall(function()
    player.PlayerGui:FindFirstChild("AdminPanel"):Destroy()
end)

local gui = Instance.new("ScreenGui")
gui.Name = "AdminPanel"
gui.ResetOnSpawn = false
gui.Parent = player.PlayerGui

-- Frame principal
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 600, 0, 400)
frame.Position = UDim2.new(0.5, -300, 0.5, -200)
frame.BackgroundColor3 = Color3.fromRGB(70, 90, 110)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
frame.Parent = gui

-- Bordure rouge
local border = Instance.new("UIStroke")
border.Color = Color3.fromRGB(220, 50, 50)
border.Thickness = 4
border.Parent = frame

-- Titre
local title = Instance.new("TextLabel")
title.Size = UDim2.new(0.7, 0, 0, 40)
title.Position = UDim2.new(0, 10, 0, 5)
title.BackgroundTransparency = 1
title.Text = "ADMIN PANEL"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextSize = 24
title.Font = Enum.Font.GothamBold
title.Parent = frame

-- Bouton X
local closeBtn = Instance.new("TextButton")
closeBtn.Size = UDim2.new(0, 40, 0, 40)
closeBtn.Position = UDim2.new(1, -50, 0, 5)
closeBtn.BackgroundColor3 = Color3.fromRGB(180, 50, 50)
closeBtn.Text = "X"
closeBtn.TextColor3 = Color3.white
closeBtn.TextSize = 20
closeBtn.Font = Enum.Font.GothamBold
closeBtn.Parent = frame

-- Zone de commande
local cmdFrame = Instance.new("Frame")
cmdFrame.Size = UDim2.new(0.65, 0, 1, -60)
cmdFrame.Position = UDim2.new(0, 10, 0, 50)
cmdFrame.BackgroundColor3 = Color3.fromRGB(50, 60, 70)
cmdFrame.BorderSizePixel = 0
cmdFrame.Parent = frame

local cmdBorder = Instance.new("UIStroke")
cmdBorder.Color = Color3.fromRGB(220, 50, 50)
cmdBorder.Thickness = 2
cmdBorder.Parent = cmdFrame

-- Type Command
local cmdLabel = Instance.new("TextLabel")
cmdLabel.Size = UDim2.new(1, 0, 0, 25)
cmdLabel.Position = UDim2.new(0, 5, 0, 5)
cmdLabel.BackgroundTransparency = 1
cmdLabel.Text = "Type Command"
cmdLabel.TextColor3 = Color3.white
cmdLabel.TextSize = 14
cmdLabel.Font = Enum.Font.Gotham
cmdLabel.TextXAlignment = Enum.TextXAlignment.Left
cmdLabel.Parent = cmdFrame

-- Input de commande
local cmdInput = Instance.new("TextBox")
cmdInput.Size = UDim2.new(1, -10, 0, 30)
cmdInput.Position = UDim2.new(0, 5, 0, 30)
cmdInput.BackgroundColor3 = Color3.fromRGB(40, 50, 60)
cmdInput.BorderSizePixel = 0
cmdInput.Text = ""
cmdInput.PlaceholderText = "Enter command"
cmdInput.TextColor3 = Color3.white
cmdInput.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
cmdInput.TextSize = 12
cmdInput.Font = Enum.Font.Gotham
cmdInput.Parent = cmdFrame

-- Fonction pour cr√©er un bouton de commande
local function createCmdButton(icon, text, position, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.48, 0, 0, 40)
    btn.Position = position
    btn.BackgroundColor3 = Color3.fromRGB(40, 50, 60)
    btn.BorderSizePixel = 0
    btn.Text = ""
    btn.Parent = cmdFrame

    local btnBorder = Instance.new("UIStroke")
    btnBorder.Color = Color3.fromRGB(100, 200, 100)
    btnBorder.Thickness = 2
    btnBorder.Parent = btn

    local iconLabel = Instance.new("TextLabel")
    iconLabel.Size = UDim2.new(0, 30, 1, 0)
    iconLabel.Position = UDim2.new(0, 5, 0, 0)
    iconLabel.BackgroundTransparency = 1
    iconLabel.Text = icon
    iconLabel.TextColor3 = Color3.white
    iconLabel.TextSize = 20
    iconLabel.Font = Enum.Font.Gotham
    iconLabel.Parent = btn

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, -40, 1, 0)
    textLabel.Position = UDim2.new(0, 35, 0, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = ";" .. text
    textLabel.TextColor3 = Color3.white
    textLabel.TextSize = 14
    textLabel.Font = Enum.Font.Gotham
    textLabel.TextXAlignment = Enum.TextXAlignment.Left
    textLabel.Parent = btn

    btn.MouseButton1Click:Connect(callback)
    return btn
end

-- Boutons de commandes (comme dans l'image)
createCmdButton("üöÄ", "rocket", UDim2.new(0, 5, 0, 70), function()
    cmdInput.Text = ";rocket"
end)

createCmdButton("üèÉ", "ragdoll", UDim2.new(0.52, 0, 0, 70), function()
    cmdInput.Text = ";ragdoll"
end)

