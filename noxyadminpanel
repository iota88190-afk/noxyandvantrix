    cmdInput.Text = ";tiny"
end)

createCmdButton("üëª", "jumpscare", UDim2.new(0, 5, 0, 270), function()
    cmdInput.Text = ";jumpscare"
end)

createCmdButton("üß†", "morph", UDim2.new(0.52, 0, 0, 270), function()
    cmdInput.Text = ";morph"
end)

-- Liste des joueurs
local playerFrame = Instance.new("Frame")
playerFrame.Size = UDim2.new(0.3, 0, 1, -60)
playerFrame.Position = UDim2.new(0.68, 0, 0, 50)
playerFrame.BackgroundColor3 = Color3.fromRGB(50, 60, 70)
playerFrame.BorderSizePixel = 0
playerFrame.Parent = frame

local playerBorder = Instance.new("UIStroke")
playerBorder.Color = Color3.fromRGB(220, 50, 50)
playerBorder.Thickness = 2
playerBorder.Parent = playerFrame

-- Scroll pour les joueurs
local playerScroll = Instance.new("ScrollingFrame")
playerScroll.Size = UDim2.new(1, -10, 1, -10)
playerScroll.Position = UDim2.new(0, 5, 0, 5)
playerScroll.BackgroundTransparency = 1
playerScroll.BorderSizePixel = 0
playerScroll.ScrollBarThickness = 8
playerScroll.Parent = playerFrame

-- Fonction pour mettre √† jour la liste des joueurs
local function updatePlayerList()
    for _, child in pairs(playerScroll:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end

    local yPos = 0
    for _, plr in pairs(Players:GetPlayers()) do
        local playerBtn = Instance.new("TextButton")
        playerBtn.Size = UDim2.new(1, -10, 0, 30)
        playerBtn.Position = UDim2.new(0, 0, 0, yPos)
        playerBtn.BackgroundColor3 = Color3.fromRGB(40, 50, 60)
        playerBtn.BorderSizePixel = 0
        playerBtn.Text = plr.Name
        playerBtn.TextColor3 = Color3.white
        playerBtn.TextSize = 12
        playerBtn.Font = Enum.Font.Gotham
        playerBtn.Parent = playerScroll

        local playerBtnBorder = Instance.new("UIStroke")
        playerBtnBorder.Color = Color3.fromRGB(100, 100, 100)
        playerBtnBorder.Thickness = 1
        playerBtnBorder.Parent = playerBtn

        playerBtn.MouseButton1Click:Connect(function()
            local currentCmd = cmdInput.Text
            if currentCmd:find(";") then
                cmdInput.Text = currentCmd .. " " .. plr.Name
            end
        end)

        yPos = yPos + 35
    end

    playerScroll.CanvasSize = UDim2.new(0, 0, 0, yPos)
end

-- Variables pour les commandes
local rocketEnabled = false
local balloonEnabled = false
local nightvisionEnabled = false

-- Ex√©cution des commandes
cmdInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local cmd = cmdInput.Text:lower()
        local char = player.Character
        if not char then return end

        -- Rocket
        if cmd:find(";rocket") then
            if not rocketEnabled then
                rocketEnabled = true
                local rocket = Instance.new("Part")
                rocket.Name = "Rocket"
                rocket.Size = Vector3.new(2, 6, 2)
                rocket.Material = Enum.Material.Neon
                rocket.BrickColor = BrickColor.new("Bright red")
                rocket.Shape = Enum.PartType.Cylinder
                rocket.CanCollide = false
                rocket.Parent = char

                local weld = Instance.new("WeldConstraint")
                weld.Part0 = char.HumanoidRootPart
                weld.Part1 = rocket
                weld.Parent = rocket

                local fire = Instance.new("Fire")
                fire.Size = 15
                fire.Heat = 20
                fire.Parent = rocket

                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
                bodyVelocity.Velocity = Vector3.new(0, 50, 0)
                bodyVelocity.Parent = char.HumanoidRootPart
            end
        end

        -- Balloon
        if cmd:find(";balloon") then
            if not balloonEnabled then
                balloonEnabled = true
                local balloon = Instance.new("Part")
                balloon.Name = "Balloon"
                balloon.Size = Vector3.new(4, 4, 4)
                balloon.Material = Enum.Material.Neon
                balloon.BrickColor = BrickColor.new("Bright red")
                balloon.Shape = Enum.PartType.Ball
                balloon.CanCollide = false
                balloon.Parent = char

                local weld = Instance.new("WeldConstraint")
                weld.Part0 = char.HumanoidRootPart
                weld.Part1 = balloon
                weld.Parent = balloon

                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
                bodyVelocity.Velocity = Vector3.new(0, 30, 0)
                bodyVelocity.Parent = char.HumanoidRootPart
            end
        end

        -- Nightvision
        if cmd:find(";nightvision") then
            nightvisionEnabled = not nightvisionEnabled
            game.Lighting.Brightness = nightvisionEnabled and 2 or 1
        end

        -- Ragdoll
        if cmd:find(";ragdoll") then
            if char:FindFirstChild("Humanoid") then
                char.Humanoid.PlatformStand = true
                for _, joint in pairs(char:GetDescendants()) do
                    if joint:IsA("Motor6D") then
                        joint:Destroy()
                    end
                end
            end
        end

        cmdInput.Text = ""
    end
end)

-- Events
closeBtn.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

-- Mise √† jour initiale
updatePlayerList()

-- Mise √† jour quand un joueur rejoint/quitte
Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoving:Connect(updatePlayerList)

print("üéÆ ADMIN PANEL LOADED!")
print("üìù Type commands or click buttons!")
